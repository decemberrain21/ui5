<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="odatabasic.controller.Buyer_Detail">
	<App>
		<pages>
			<Page title="Buyer Detail" showNavButton="true" navButtonPress="GoBuyerList">
				<content>
				    <VBox width="100%" direction="Column" id="__vbox0">
				        <items>
				            <Text text="Email: {EMAILADDRESS}" id="__text0"/>
				            <Text text="Company: {COMPANYNAME}" id="__text1"/>
				            <Text text="City: {CITY}" id="__text2"/>
				        </items>
				    </VBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>